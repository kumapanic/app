  <meta name="generator" content="Hugo 0.79.0">
  <title>Blogダッシュボード</title>

  <!-- Bootstrap core CSS -->
  <link href=https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css rel="stylesheet"
    integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">

  <!-- Favicons -->
  <link rel="apple-touch-icon" href="/docs/5.0/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
  <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
  <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
  <link rel="manifest" href="/docs/5.0/assets/img/favicons/manifest.json">
  <link rel="mask-icon" href="/docs/5.0/assets/img/favicons/safari-pinned-tab.svg" color="#7952b3">
  <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon.ico">
  <meta name="theme-color" content="#7952b3">
  <!-- Custom styles for this template -->
  <%- include("../_share/stylesheet.ejs") %>
</head>

<body>

  <%- include("../_share/header.ejs") %>

    <div class="container-fluid">
      <div class="row">
        <%- include("../_share/nav.ejs") %>

          <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
            <div
              class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
              <h1 class="h2">記事投稿</h1>
            </div>
            <% var original=locals.original || {}; var message=locals.message || {}%>
              <form action="/account/article-posting/execute" method="POST" class="p-3 posting-form" enctype="multipart/form-data">
                <% if (locals.message) { %>
                  <div class="alert alert-danger">
                    入力内容に問題があります。確認して再度お試しください。
                  </div>
                  <% } %>
                    <div class="form-group">
                      <label for="url">URL<span class="posting-reqiured">（必須）</span><% if (message.url) { %><span>※<%= message.url %></span>
                          <% } %></label>
                      <input class="form-control" id="url" name="url" type="text" value="<%= original.url %>">
                    </div>
                    <div class="form-group">
                      <label for="file">イメージ<span class="posting-reqiured">（必須）</span><% if (message.file) { %><span>※<%= message.file %></span>
                          <% } %></label>
                      <input class="form-control" id="file" name="file" type="file" accept="">
                    </div>
                    <div class="form-group">
                      <label for="overview">記事概要<span class="posting-reqiured">（必須）</span><% if (message.overview) { %><span>※<%= message.overview %></span>
                          <% } %></label>
                      <textarea class="form-control" name="overview" id="overview" cols="30"
                        rows="3"><%= original.subcontent %></textarea>
                    </div>
                    <div class="form-group">
                      <label for="title">タイトル<span class="posting-reqiured">（必須）</span><% if (message.title) { %><span>※<%= message.title %></span>
                          <% } %></label>
                      <input class="form-control" name="title" id="title" type="text" value="<%= original.title %>">
                    </div>
                    <div class="form-group">
                      <label for="content">コンテンツ<span class="posting-reqiured">（必須）</span><% if (message.content) { %><span>※<%= message.content %></span>
                          <% } %></label>
                      <textarea class="form-control" name="content" id="content" cols="30"
                        rows="5"><%= original.content1 %></textarea>
                    </div>
                    <div class="form-group">
                      <label for="subtitle1">サブタイトル1</label>
                      <input class="form-control" name="subtitle1" id="subtitle1" type="text"
                        value="<%= original.subtitle1 %>">
                    </div>
                    <div class="form-group">
                      <label for="subcontent1">サブコンテンツ１</label>
                      <textarea class="form-control" name="subcontent1" id="subcontent1" cols="30"
                        rows="5"><%= original.content2 %></textarea>
                    </div>
                    <div class="form-group">
                      <label for="subtitle2">サブタイトル２</label>
                      <input class="form-control" name="subtitle2" id="subtitle2" type="text"
                        value="<%= original.subtitle2 %>">
                    </div>
                    <div class="form-group">
                      <label for="subcontent2">サブコンテンツ２</label>
                      <textarea class="form-control" name="subcontent2" id="subcontent2" cols="30"
                        rows="5"><%= original.content3 %></textarea>
                    </div>
                    <div class="form-group">
                      <label for="subtitle3">サブタイトル３</label>
                      <input class="form-control" name="subtitle3" id="subtitle3" type="text"
                        value="<%= original.title3 %>">
                    </div>
                    <div class="form-group">
                      <label for="subcontent3">サブコンテンツ３</label>
                      <textarea class="form-control" name="subcontent3" id="subcontent3" cols="30"
                        rows="5"><%= original.content4 %></textarea>
                    </div>
                    <div class="form-group">
                      <label for="subtitle4">サブタイトル４</label>
                      <input class="form-control" name="subtitle4" id="subtitle4" type="text"
                        value="<%= original.subtitle4 %>">
                    </div>
                    <div class="form-group">
                      <label for="subcontent4">サブコンテンツ４</label>
                      <textarea class="form-control" name="subcontent4" id="subcontent4" cols="30"
                        rows="5"><%= original.content5 %></textarea>
                    </div>
                    <div class="form-group">
                      <label for="subtitle5">サブタイトル５</label>
                      <input class="form-control" name="subtitle5" id="subtitle5" type="text"
                        value="<%= original.subtitle5 %>">
                    </div>
                    <div class="form-group">
                      <label for="subcontent5">サブコンテンツ５</label>
                      <textarea class="form-control" name="subcontent5" id="subcontent5" cols="30"
                        rows="5"><%= original.content6 %></textarea>
                    </div>
                    <div class="mt-3 mb-3">
                      <input class="btn btn-primary text-end" type="submit" value="確認">
                    </div>
              </form>
          </main>
      </div>
    </div>
    <%- include("../_share/javascript.ejs") %>
